<?import javafx.geometry.Insets?>

<AnchorPane prefHeight="797.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="imix.imix.PrimaryController">
   <children>
      <GridPane prefHeight="797.0" prefWidth="640.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="312.0" minWidth="10.0" prefWidth="239.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="178.0" minWidth="10.0" prefWidth="170.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="306.0" minWidth="10.0" prefWidth="246.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints maxHeight="528.0" minHeight="10.0" prefHeight="497.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <VBox spacing="20.0">
               <children>
                  <ImageView fx:id="imageT" fitHeight="216.0" fitWidth="325.0" onMouseClicked="#folderOpenActionTop" pickOnBounds="true" preserveRatio="true" />
                  <ImageView fx:id="imageB" fitHeight="216.0" fitWidth="325.0" onMouseClicked="#folderOpenActionBottom" pickOnBounds="true" preserveRatio="true" />
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </VBox>
            <Button fx:id="imageMixButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#imageMixing" text="画像結合" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
                  <ImageView fx:id="mixedImage" fitHeight="216.0" fitWidth="325.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
